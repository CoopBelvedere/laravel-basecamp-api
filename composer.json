{
    "name": "coopbelvedere/laravel-basecamp-api",
    "description": "API Wrapper for Basecamp3",
    "require": {
        "php": ">=7.0",
        "ext-fileinfo": "*",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "kevinrob/guzzle-cache-middleware": "^3.0",
        "illuminate/cache": "^5.5|^6.0|^7.0",
        "illuminate/filesystem": "^5.5|^6.0|^7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Bastien",
            "email": "mbastien@coopbelvedere.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Belvedere\\Basecamp\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belvedere\\Basecamp\\BasecampServiceProvider"
            ],
            "aliases": {
                "Basecamp": "Belvedere\\Basecamp\\BasecampFacade"
            }
        }
    }
}
